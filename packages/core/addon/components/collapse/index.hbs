<div collapse-change={{this.getCollapseIsOpen}} style={{this.containerStyles}} class={{rw this.COLLAPSE
	this.getCollapseClassName}}>
	<div class={{rw this.COLLAPSE_BODY}} style={{this.contentsStyles}} aria-hidden={{if
		this.getCollapseKCMounted 'true' 'false' }} {{did-insert this.inputElementRef}} {{did-update this.componentDidUpdate
		this.getCollapseTD this.isOpen}}>
		{{#if this.shouldRenderChildren}}
			{{yield}}
		{{/if}}
	</div>
</div>