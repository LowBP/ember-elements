<Button @onClick={{action 'onClickButton'}}>
  {{#if this.isOpen}}
    {{this.hide}}
  {{else}}
    {{this.show}}
  {{/if}}
  {{this.build}}
</Button>
<Collapse
  @isOpen={{this.isOpen}}
  @keepChildrenMounted={{this.keepChildrenMounted}}
>
  <pre class='bp3-code-block'>
    {{this.collapseText}}
  </pre>
</Collapse>

<Button
  @props={{this.propsObject}}
  @onClick={{action 'onClick'}}
  @onKeyUp={{action 'onKeyUp'}}
  @onKeyDown={{action 'onKeyDown'}}
>
  {{this.text}}
</Button>
<Icon
  @icon='calendar'
  @iconSize={{20}}
  @intent='primary'
  @color='green'
  @title='calendar'
/>

<ButtonGroup
  @minimal={{this.minimal}}
  @large={{this.large}}
  @vertical={{this.vertical}}
  @fill={{this.fill}}
  @alignText={{this.alignText}}
  style='min-width:200px'
>
  <Button @icon='database' @type='button' />
  <Button @icon='function' @type='button' />
  <Button @icon='cog' @type='button' />
</ButtonGroup>
<Callout @props={{this.propsValue}} @title='Visually important content'>
  {{this.calloutText}}
</Callout>

<Card @elevation={{this.elevation}}>
  {{this.elevationText}}
</Card>

<ProgressBar @value={{0.7}} />

<HtmlTable
  @bordered={{true}}
  @className='foo'
  @interactive={{true}}
  @striped={{true}}
>
  <thead>
    <tr>
      <th>
      </th>
      <th>
      </th>
      <th>
      </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
      </td>
      <td>
      </td>
      <td>
      </td>
    </tr>
    <tr>
      <td>
      </td>
      <td>
      </td>
      <td>
      </td>
    </tr>
    <tr>
      <td>
      </td>
      <td>
      </td>
      <td>
      </td>
    </tr>
  </tbody>
</HtmlTable>

<Spinner
  @size={{this.size}}
  @value={{this.value}}
  @intent='danger'
  @className='ee-small'
/>

<FormGroup
  @helperText='Helper text with details...'
  @label='Label A'
  @labelFor='text-input'
  @labelInfo='(required)'
  @intent='primary'
  @inline={{true}}
  style='color:red'
>
  <input placeholder='Placeholder text' >
</FormGroup>

<ControlGroup @fill={{true}} @vertical={{false}}>
  <Button @icon='filter' />
  <Button @icon='filter' />
</ControlGroup>

<Label />

<Checkbox
  @label='Enabled'
  @onChange={{action this.handleEnabledChange}}
  @defaultIndeterminate={{true}}
/>
<RadioGroup @label='Determine lunch'>
  <Radio @label='Soup' @value='one' name='radio1' @inline={{true}} />
  <Radio @label='Salad' @value='two' name='radio1' @inline={{true}} />
</RadioGroup>

<HtmlSelect @options={{this.options}} />

<div>
  <Label>
    {{this.switchLabel}}
  </Label>
  <Switch @labelElement='<strong>Enabled</strong>' @inline={{true}} />
  <Switch @labelElement='<em>Public</em>' @inline={{true}} />
  <Switch
    @labelElement='<u>Cooperative</u>'
    defaultChecked={{true}}
    @inline={{true}}
  />
  <Switch
    @labelElement='Containing Text'
    @innerLabelChecked='on'
    @innerLabel='off'
    @inline={{true}}
  />
</div>
<FileInput
  @buttonText='hii'
  @large={{true}}
  @hasSelection={{true}}
  @text='Choose file...'
/>

<InputGroup>
  <Button>
    {{this.textIG}}
  </Button>
</InputGroup>
<TextArea @growVertically={{true}} @value={{this.value1}} @intent='primary' />

<Tag @icon='home' @large={{true}}>
  {{this.tagText}}
</Tag>

<TagInput
  @onChange={{action this.handleChange}}
  @values={{this.values}}
  @inputValue='a'
  {{! @leftIcon='user' }}
  {{! @placeholder='Separate values with commas...' }}
/>

<NumericInput
  @disabled={{this.disabled}}
  @large={{this.large}}
  @fill={{this.fill}}
  @leftIcon={{this.leftIcon}}
  @allowNumericCharactersOnly={{this.allowNumericCharactersOnly}}
  @selectAllOnFocus={{this.selectAllOnFocus}}
  @selectAllOnIncrement={{this.selectAllOnIncrement}}
  @min={{this.min}}
  @max={{this.max}}
  @intent={{this.intent}}
  @buttonPosition={{this.buttonPosition}}
  @value={{this.valueNI}}
  @didPasteEventJustOccur={{true}}
/>

<ButtonGroup @minimal={{true}} @vertical={{this.vertical}}>
  <Button @icon='folder-close' />
  <Button @icon='edit' />
  <Divider />
  <Button @icon='folder-new' />
  <Button @icon='trash' />
  <Divider />
  <Button @icon='add' />
  <Button @icon='remove' />
</ButtonGroup>